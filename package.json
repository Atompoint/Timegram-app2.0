{
  "name": "Timegram",
  "version": "0.2.17",
  "author": "Atompoint",
  "description": "Smart Time Tracking",
  "private": true,
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^4.19.5",
    "auto-launch": "^5.0.5",
    "electron-active-window": "^0.0.6",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^5.0.1",
    "firebase": "8.10.1",
    "firestore-size": "^2.0.7",
    "moment": "^2.29.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron": "^11.2.1",
    "electron-builder": "^22.9.1",
    "prettier": "^2.2.1",
    "wait-on": "^6.0.1"
  },
  "main": "public/main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none yarn start\" \"yarn electron:start\"",
    "electron:build": "yarn build && electron-builder -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on tcp:3000 && electron .",
    "electron:publish": "yarn build && electron-builder -c.extraMetadata.main=build/main.js --win --publish always"
  },
  "build": {
    "extends": null,
    "appId": "com.timegram.electron",
    "directories": {
      "output": "electron-build",
      "buildResources": "build-assets"
    },
    "win": {
      "icon": "build/icon.ico",
      "legalTrademarks": "(c) 2020 Timegram",
      "publisherName": "Timegram",
      "publish": [
        "github"
      ]
    },
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
